{
  "type": "object",
  "properties": {
    "firstatsite": {
      "description": "First ever diagnosis by site | First (earliest) diagnosis for each 'site_text'. An individual may have multipe firstatsite=1 (e.g. one for breast, one for skin, etc)",
      "type": ["string"],
      "maxLength": 3
    },
    "confirmed": {
      "description": "Registry confirmed cancer",
      "type": ["string"],
      "maxLength": 3
    },
    "incident": {
      "description": "Incident or prevalent cancer based on entry date to study | Based on diagnosis date and recruitment to study (based on entrydate From mailing.General table)",
      "type": ["string"],
      "enum": ["0", "1", "9"],
      "enumDescriptions" :[
        "0: prevalent",
        "1: incident",
        "9: not known"
      ]
    },
    "StudyID": {
      "description": "Internal Study ID",
      "type": ["integer"],
      "minimum": 0,
      "maximum": 499994
    },
    "Reginfo_GroupDateSite": {
      "description": "Indicator of Registry Grouping of episode by site within a date range | groups (registry source) diagnosis together if same 'site_text' and diagnosis date (is similar): i.e. multiple reports (DCIS vs invasive; left vs right) at essentially the same time",
      "type": ["integer"]
    },
    "Reginfo_Clusterino": {
      "description": "Registry one or more independent tumours (identified by ICD code/side) within grouped episode | Within the same 'Reginfo_GroupDateSite' orders the tumours",
      "type": ["integer"]
    },
    "Report_GroupDateSite": {
      "description": "Indicator of Participant Reported Grouping of episode by site, only defined if no corresponding registry data",
      "type": ["integer"]
    },
    "Report_Cluster": {
      "description": "Reported one or more independent tumours (identified by ICD code/side) within grouped episode",
      "type": ["integer"]
    },
    "source": {
      "description": "Registry notification or Questionnaire notification (Deaths Flagging source only included where no other notification of cancer) | Deaths Flagging source only included where no other notification of cancer",
      "type": ["string"],
      "maxLength": 20
    },
    "diagdate": {
      "description": "Selected Diagnosis date",
      "type": ["string"],
      "format": "date-time",
      "maxLength": 23
    },
    "site_text": {
      "description": "Site of Cancer | ICDm 85403 and 85423 site changed from 'breast' to 'breast pagets' for any that have no receptor status (ER, PR, HER2)",
      "type": ["string"],
      "maxLength": 30
    },
    "side": {
      "description": "Laterality",
      "type": ["string"],
      "enum": ["1", "2", "3", "4"],
      "enumDescriptions" :[
        "1: Left",
        "2: Right",
        "3: Both",
        "4: not known",
        "null: missing or not applicable"
      ]
    },
    "regdate": {
      "description": "Registry diagnosis date",
      "type": ["string"],
      "format": "date-time",
      "maxLength": 23
    },
    "repdate": {
      "description": "Participant Reported diagnosis date",
      "type": ["string"],
      "format": "date-time",
      "maxLength": 23
    },
    "S_repdate": {
      "description": "Full unique Participant Reported source of diagnosis",
      "type": ["string"],
      "maxLength": 100
    },
    "rep_source": {
      "description": "Questionnaire source of diagnosis | if 'source' from Registry then linked Participant Source otherwise same as 'source'",
      "type": ["string"],
      "maxLength": 100
    },
    "ICDt": {
      "description": "International classification of disease (C..., D...). Diagnostic and procedure coding system | Includes local coding: 4th character 'Z' means no information to determine 4th character. Where source is Histopathology Report code is derived",
      "type": ["string"],
      "maxLength": 5
    },
    "ICDt_v": {
      "description": "Intl classification of disease - version",
      "type": ["string"],
      "maxLength": 15
    },
    "ICDm": {
      "description": "5 digit morphology code. First 4 digits indicate specific histological term, 5th digit behaviour code (malignant,in situ, etc) | Where source is Histopathology Report code is derived",
      "type": ["string"],
      "maxLength": 5
    },
    "er_Status": {
      "description": "ER Receptor Status",
      "type": ["string", "null"],
      "maxLength": 10
    },
    "pr_Status": {
      "description": "PR Receptor Status",
      "type": ["string", "null"],
      "maxLength": 10
    },
    "her2_Status": {
      "description": "HER2 Receptor Status",
      "type": ["string", "null"],
      "maxLength": 10
    },
    "ck56_Status": {
