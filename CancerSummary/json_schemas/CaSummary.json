      "maxLength": 10
    },
    "ki67_status": {
      "description": "ki67 Receptor status",
      "type": ["string", "null"],
      "maxLength": 10
    },
    "grade": {
      "description": "Pathology grade",
      "type": ["string", "null"],
      "maxLength": 15
    },
    "stage": {
      "description": "Pathology Stage",
      "type": ["string", "null"],
      "maxLength": 30
