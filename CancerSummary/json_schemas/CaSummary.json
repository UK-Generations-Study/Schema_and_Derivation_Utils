      "enum":["A", "B", "3", null],
      "enumDescriptions": [
          "A: First laterality of bilateral",
          "B: Second laterality",
          "3: Bilateral recorded as one event",
          "null: missing or not applicable"
          ]
    },
    "Screen_Detected": {
